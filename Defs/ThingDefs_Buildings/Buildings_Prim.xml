  	<ThingDef ParentName="BuildingBase">
		<defName>LabyrinthScanner</defName>
		<label>Labyrinth Scanner</label>
		<description>Scanner equipment used to find labyrinths.</description>

		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Building/Production/DeepDrill</texPath>
			<drawSize>(2,2)</drawSize>
			<drawRotated>false</drawRotated>
			<shadowData>
				<volume>(0.52, 0.53, 0.09)</volume>
				<offset>(0,0,-0.43)</offset>
			</shadowData>
		</graphicData>
		<size>(1,1)</size>
		<canBeUsedUnderRoof>false</canBeUsedUnderRoof>
		<fillPercent>0.15</fillPercent>
		<altitudeLayer>Building</altitudeLayer>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,6,1)</interactionCellOffset>
		<passability>PassThroughOnly</passability>
		
		<pathCost>50</pathCost>
		<socialPropernessMatters>true</socialPropernessMatters>
		<designationCategory>Misc</designationCategory>
		<costList>
			<Steel>500</Steel>
			<Plasteel>250</Plasteel>
			<ComponentIndustrial>30</ComponentIndustrial>
			<ComponentSpacer>10</ComponentSpacer>
		</costList>
		<comps>
			<li Class="PRimUtility.CompProperties_LabyrinthKey" >
				<LabyrinthDefNameQueue>
					<li>ExampleLabyrinth</li>
				</LabyrinthDefNameQueue>
				<LabyrinthDistances>
					<li>5</li>
				</LabyrinthDistances>
				<UseTargetBiome>false</UseTargetBiome>
				<CycleLabyrinths>false</CycleLabyrinths>
				<targetBiome>null</targetBiome>				
			</li>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>2000</basePowerConsumption>
			</li>
			<li Class="CompProperties_Mannable">
				<manWorkType>Violent</manWorkType>
		    </li>
		</comps>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<MarketValue>350</MarketValue>
			<Mass>7.5</Mass>
			<JoyGainFactor>1.2</JoyGainFactor>
		</statBases>
		<placeWorkers>
			<li>PlaceWorker_NotUnderRoof</li>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
	</ThingDef>
</Defs>
